[Unit]
Description=BluTunnel Reverse Tunnel
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/opt/blutunnel
EnvironmentFile=-/etc/blutunnel/blutunnel.env
ExecStart=/usr/bin/python3 /opt/blutunnel/blutunnel.py
Restart=always
RestartSec=3
LimitNOFILE=1000000

[Install]
WantedBy=multi-user.target
